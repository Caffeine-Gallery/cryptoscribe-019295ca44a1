type Time = int;
type Result = 
 variant {
   err: text;
   ok: Profile;
 };
type Profile = 
 record {
   bio: text;
   picture: opt blob;
   username: text;
 };
type Post = 
 record {
   authorUsername: text;
   body: text;
   id: nat;
   timestamp: Time;
   title: text;
 };
service : {
  createPost: (text, text) -> (nat);
  getPosts: () -> (vec Post) query;
  getProfile: () -> (Result);
  updateProfile: (text, text, opt vec nat8) -> ();
}
